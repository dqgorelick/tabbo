{
	"manifest_version": 2,
	"browser_specific_settings": {
		"gecko": {
			"id": "tabbo@tabbo.com"
		}
	},
	"browser_action": {
		"default_icon": "images/tabbo128.png",
		"default_popup": "popup.html"
	},
	"name": "Tabbo",
	"description": "Tab Management Hotkeys",
	"short_name": "Tabbo â€“ Tab Management Hotkeys",
	"version": "0.11.0",
	"icons": {
		"16": "images/tabbo16.png",
		"48": "images/tabbo48.png",
		"128": "images/tabbo128.png"
	},
	"content_scripts": [],
	"background": {
		"scripts": [
			"scripts/background.bundle.js"
		],
		"persistent": false
	},
	"permissions": [
		"tabs",
		"activeTab",
		"<all_urls>"
	],
	"commands": {
		"MOVE_RIGHT": {
			"suggested_key": {
				"default": "Ctrl+Shift+L",
				"mac": "MacCtrl+Shift+L"
			},
			"description": "Move current tab to the right"
		},
		"MOVE_LEFT": {
			"suggested_key": {
				"default": "Ctrl+Shift+H",
				"mac": "MacCtrl+Shift+H"
			},
			"description": "Move current tab to the left"
		},
		"POP_TAB": {
			"suggested_key": {
				"default": "Ctrl+Shift+J",
				"mac": "MacCtrl+Shift+J"
			},
			"description": "Pop current tab to new window"
		},
		"PUSH_TAB": {
			"suggested_key": {
				"default": "Ctrl+Shift+K",
				"mac": "MacCtrl+Shift+K"
			},
			"description": "Push current tab to another window"
		}
	},
	"content_security_policy": "script-src 'self' ./; object-src 'self'"
}
